      <rdf:Description rdf:nodeID="N3d1c76613e0d4e0db2edd294435c7e62">
        <rdf:value>Household appliances</rdf:value>
        <dcam:memberOf rdf:resource="http://purl.org/dc/terms/LCSH"/>
